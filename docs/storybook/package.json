{
  "name": "taskbox",
  "version": "0.2.0",
  "description": "Starter template to get up and running quickly with Angular and Storybook",
  "author": "Chromatic <https://chromatic.com/>",
  "repository": {
    "type": "git",
    "url": "https://github.com/chromaui/intro-storybook-angular-template"
  },
  "bugs": {
    "url": "https://github.com/chromaui/intro-storybook-angular-template/issues"
  },
  "license": "MIT",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "storybook": "ng run taskbox:storybook",
    "build-storybook": "ng run taskbox:build-storybook",
    "init-msw": "msw init public/"
  },
  "private": true,
  "dependencies": {
    "@acme/ds-angular": "workspace:*",
    "@acme/ds-css": "workspace:*",
    "@angular/animations": "^19.2.15",
    "@angular/common": "^19.2.15",
    "@angular/compiler": "^19.2.15",
    "@angular/core": "^19.2.15",
    "@angular/forms": "^19.2.15",
    "@angular/platform-browser": "^19.2.15",
    "@angular/platform-browser-dynamic": "^19.2.15",
    "@angular/router": "^19.2.15",
    "angular": "link:@storybook/addon-docs/angular",
    "class-variance-authority": "^0.7.1",
    "react-markdown": "^10.1.0",
    "rxjs": "~7.8.0",
    "tslib": "^2.8.1",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.0.6",
    "@angular/cli": "^19.0.6",
    "@angular/compiler-cli": "^19.2.15",
    "@compodoc/compodoc": "^1.1.26",
    "@storybook/addon-docs": "^9.1.1",
    "@storybook/addon-onboarding": "^9.1.1",
    "@storybook/angular": "^9.1.7",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.19.17",
    "jasmine-core": "~5.4.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "storybook": "^9.1.1",
    "typescript": "~5.6.2"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  }
}